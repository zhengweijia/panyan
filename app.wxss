/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background-color: #f4f4f4;
} 

button.disabled {
    background-color: #aaa;
    border: 1px solid #ababab;
    color: #fff;
}

.input-section {
    background-color: #fff;
    color: #989898;
    font-size: 30rpx;
    box-sizing: border-box;
    width: 100%;
    border: 1px solid #aaa;
    padding: 25rpx;
    border-radius: 10rpx;
}
input.default {
  width: 100%;
  color: #000;
}

.title-label {
  display: block;
  margin: 0 0 20rpx;
  font-size: 30rpx;
  color: #989898;

}