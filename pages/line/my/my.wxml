<!--my.wxml-->
<view class="my">
  <view class="top">
    <view class="circle">
      <view>您已经完成</view>
      <view class="num"><text >{{viewData.lineNum}} </text>条</view>
      <view>攀爬路线</view>
    </view>

    <view class="all-money"><text>已获得奖金：￥{{viewData.money}}</text></view>
  </view>

  <view class="weui-panel weui-panel_access">
    <view class="weui-panel__bd">
      <navigator url="" wx:for="{{viewData.resultList}}" wx:key="id" class="weui-media-box weui-media-box_appmsg detail" hover-class="weui-cell_active">
        <view class="weui-media-box__hd weui-media-box__hd_in-appmsg diffcult-{{item.line.lineDifficulty.zoomIndex}}">
        </view>
        <view class="weui-media-box__bd weui-media-box__bd_in-appmsg text">
          <view class="weui-media-box__title">{{item.line.name}} 难度{{item.line.lineDifficulty.usa}}  线路总奖金<text class="red">￥{{item.line.bonus}}</text></view>
          <view class="weui-media-box__desc">当前完成线路人数 <text class="red"> {{item.line.finish_num}}</text></view>
          <view class="weui-media-box__desc">您当前获得的奖金 <text class="red">￥{{ item.line.preMoney}}</text></view>

          <view class="weui-media-box__desc">完成时间：{{item.end_time}} 线路用时 {{item.end_time-item.start_time}}</view>
        </view>
      </navigator>

    </view>
  </view>
</view>
